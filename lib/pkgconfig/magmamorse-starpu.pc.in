prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${exec_prefix}/include

Name: magmamorse
Description: Parallel Linear Algebra for Scalable Multi-core Architectures and GPGPUs
Version: @MAGMAMORSE_VERSION_MAJOR@.@MAGMAMORSE_VERSION_MINOR@.@MAGMAMORSE_VERSION_PATCH@
Cflags: -I${includedir} 
Libs: -L${libdir} @MAGMAMORSE_PKGCONFIG_LIBS@
Libs.private: @MAGMAMORSE_PKGCONFIG_LIBS_PRIVATE@
Requires: @MAGMAMORSE_PKGCONFIG_REQUIRED@
Requires.private: @MAGMAMORSE_PKGCONFIG_REQUIRED_PRIVATE@
